{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/md5.php",
        "dest": "/public/md5.php"
      },
      {
        "src": "/makecode.php",
        "dest": "/public/makecode.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}
